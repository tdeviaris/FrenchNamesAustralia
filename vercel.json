{
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Authorization"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/glossaryE.html", "destination": "/glossary.html", "permanent": true },
    { "source": "/glossaryF.html", "destination": "/glossary.html", "permanent": true },
    { "source": "/acteursE.html", "destination": "/actors.html", "permanent": true },
    { "source": "/acteursF.html", "destination": "/actors.html", "permanent": true },
    { "source": "/cartesE.html", "destination": "/maps.html", "permanent": true },
    { "source": "/cartesF.html", "destination": "/maps.html", "permanent": true },
    { "source": "/SourcesE.html", "destination": "/methodology.html", "permanent": true },
    { "source": "/SourcesF.html", "destination": "/methodology.html", "permanent": true }
  ]
}
